<view class='body'>
  <view class='userinfo'>
    <image class='userinfo-avatar' src='{{userInfo.avatarUrl}}'></image>
    <text class='userinfo-nickname'>{{userInfo.nickName}} </text>
    <text class='userinfo-number'>毛积分: {{number}} </text>
  </view>
  <view class='list'>
    <view class="weui-cells weui-cells_after-title">
      <navigator url="../set/set" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__hd">
          <image src="../../img/shezhi.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px">
          </image>
        </view>
        <view class="weui-cell__bd">设置</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>

      </navigator>

    </view>
    <view class="weui-cells weui-cells_after-title">
      <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__hd">
          <image src="../../img/lingdang.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px">
          </image>
        </view>
        <view class="weui-cell__bd">消息通知</view>
        <view class="weui-cell__ft weui-cell__ft_in-access">2</view>

      </navigator>

    </view>
    <view class="weui-cells weui-cells_after-title">
      <navigator url="../time-line/time-line" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__hd">
          <image src="../../images/爪子01.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px">
          </image>
        </view>
        <view class="weui-cell__bd">我的时间轴</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>

      </navigator>

    </view>
    <view class="weui-cells weui-cells_after-title">
      <navigator url= "../xuzhi/xuzhi" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__hd">
          <image src="../../img/xuzhi.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px">
          </image>
        </view>
        <view class="weui-cell__bd">须知</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>

      </navigator>

    </view>
  </view>
</view>